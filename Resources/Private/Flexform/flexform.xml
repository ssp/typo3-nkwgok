<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <ROOT>
        <TCEforms>
            <sheetTitle>LLL:EXT:nkwlib/locallang_db.xml:flexform.options</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
            <settings.source>
                <TCEforms>
                    <label>LLL:EXT:nkwgok/Resources/Private/Language/locallang.xml:source</label>
                    <config>
                        <type>select</type>
                        <itemsProcFunc>Subugoe\Nkwgok\Elements\Flexform->addFields</itemsProcFunc>
                    </config>
                </TCEforms>
            </settings.source>

            <settings.altSource>
                <TCEforms>
                    <label>LLL:EXT:nkwgok/Resources/Private/Language/locallang.xml:altSource</label>
                    <config>
                        <type>input</type>
                        <size>30</size>
                    </config>
                </TCEforms>
            </settings.altSource>

            <settings.style>
                <TCEforms>
                    <label>LLL:EXT:nkwgok/Resources/Private/Language/locallang.xml:style</label>
                    <config>
                        <type>radio</type>
                        <items type="array">
                            <numIndex index="0" type="array">
                                <numIndex index="0">LLL:EXT:nkwgok/Resources/Private/Language/locallang.xml:styleTree</numIndex>
                                <numIndex index="1">tree</numIndex>
                            </numIndex>
                            <numIndex index="1" type="array">
                                <numIndex index="0">LLL:EXT:nkwgok/Resources/Private/Language/locallang.xml:styleColumn</numIndex>
                                <numIndex index="1">column</numIndex>
                            </numIndex>
                            <numIndex index="2" type="array">
                                <numIndex index="0">LLL:EXT:nkwgok/Resources/Private/Language/locallang.xml:styleMenu</numIndex>
                                <numIndex index="1">menu</numIndex>
                            </numIndex>
                        </items>
                    </config>
                </TCEforms>
            </settings.style>

            <settings.showGOKID>
                <TCEforms>
                    <label>LLL:EXT:nkwgok/Resources/Private/Language/locallang.xml:showGOKID</label>
                    <config>
                        <type>check</type>
                        <default>1</default>
                    </config>
                </TCEforms>
            </settings.showGOKID>

            <settings.omitXXX>
                <TCEforms>
                    <label>LLL:EXT:nkwgok/Resources/Private/Language/locallang.xml:omitXXX</label>
                    <config>
                        <type>check</type>
                        <default>0</default>
                    </config>
                </TCEforms>
            </settings.omitXXX>

        </el>
    </ROOT>
</T3DataStructure>
